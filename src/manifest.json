{
  "name": "Likers Blocker",
  "version": "0.0.0.3",
  "version_name": "1.0-beta.5",
  "description": "Block all visible likers of a tweet.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["script.js"],
      "css": ["style.css"]
    }
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  }
}
