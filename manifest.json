{
  "name": "Likers Blocker",
  "version": "1.0",
  "description": "Block all likers of a tweet",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["script.js"]
    }
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["declarativeContent", "tabs", "activeTab", "webNavigation"]
}
