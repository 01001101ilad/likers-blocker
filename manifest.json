{
  "name": "Likers Blocker",
  "version": "1.0",
  "description": "Block (almost) all likers of a tweet.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["script.js"],
      "css": ["style.css"]
    }
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "declarativeContent",
    "tabs",
    "activeTab",
    "webNavigation",
    "https://ichbinhier-twittertools.herokuapp.com/"
  ]
}
